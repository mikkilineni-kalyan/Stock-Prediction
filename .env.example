# Required API Keys (Core Functionality)
ALPHA_VANTAGE_KEY=your_alpha_vantage_key            # Get from: https://www.alphavantage.co/support/#api-key
NEWS_API_KEY=your_news_api_key                      # Get from: https://newsapi.org/register
FINNHUB_API_KEY=your_finnhub_key                    # Get from: https://finnhub.io/register
POLYGON_API_KEY=your_polygon_key                    # Get from: https://polygon.io/dashboard/signup

# Optional API Keys (Enhanced Features)
TWITTER_API_KEY=your_twitter_api_key                # Get from: https://developer.twitter.com/en/portal/dashboard
TWITTER_API_SECRET=your_twitter_api_secret
SEEKING_ALPHA_API_KEY=your_seeking_alpha_key        # Get from: https://seekingalpha.com/account/api_keys
SEC_API_KEY=your_sec_api_key                        # Get from: https://sec-api.io/register

# Database Configuration
DB_DIALECT=sqlite
DB_NAME=stock_prediction.db
